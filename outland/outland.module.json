{
  "requires": ["items", "map"], 
  "excludes": "",
  "title": "Модуль для Outland",
  "weight": 5, 
  "pages": {
    "/walk.php": [
      "outland_check_set",
      "outland_auto_sail",
      "outland_auto_close",
      "outland_auto_pick",
      "outland_highlight",
      "outland_hotkeys",
      "outland_pokemon_title"
    ],
    "/walk.op.php": [
      "outland_check_set",
      "outland_auto_sail",
      "outland_auto_close",
      "outland_auto_pick",
      "outland_highlight",
      "outland_hotkeys",
      "outland_pokemon_title"
    ],
    "/walk.ep.php": [
      "outland_check_set",
      "outland_auto_sail",
      "outland_auto_close",
      "outland_auto_pick",
      "outland_highlight",
      "outland_hotkeys",
      "outland_pokemon_title"
    ],
    "/walk.p.php": [
      "outland_check_set",
      "outland_auto_sail",
      "outland_auto_close",
      "outland_auto_pick",
      "outland_highlight",
      "outland_hotkeys",
      "outland_pokemon_title"
    ],
    "/walk.bp.php": [
      "outland_check_set",
      "outland_auto_sail",
      "outland_auto_close",
      "outland_auto_pick",
      "outland_highlight",
      "outland_hotkeys",
      "outland_pokemon_title"
    ]
  },
  "methods": {
    "outland_check_set": {
      "configure": {
        "set": {
          "type": "select",
          "title": "комплект",
          "options": ["test1", "test2"],
          "options": "__panel.items_get_sets_async()",
          "file": "items/items_data.js",
          "default": 0
        }
      },
      "file": "outland.js"
    },

    "outland_auto_sail": {
      "title": "Автоматически отплывать, ага",
      "configure": {
        "port": {
          "type": "select",
          "title": "порт назначения",
          "options": "__panel.map_get_ports_ids()",
          "default": "",
          "file": "map/map_sectors.js"
        }
      },
      "file": "outland.js"
    },

    "outland_auto_close": {
      "title": "Автоматически закрывать заявку",
      "file": "outland.js"
    },

    "outland_auto_pick": {
      "title": "Автоматически подбирать вещи",
      "file": "outland.js",
      "configure": {
        "type_loot": {
          "type": "checkboxes",
          "title": "Какой лут не подбирать:",
          "options": "__panel.type_loot",
          "default": {},
          "file": "outland_data.js",
          "collapsible": false,
          "description": "Можно выбрать несколько значений"
        }
      }
    },

    "outland_highlight": {
      "title": "Подкрашивать клетки, на которые можно перейти",
      "file": "outland.js"
    },

    "outland_hotkeys": {
      "title": "Быстрое передвижение стрелками на клавиатуре",
      "file": "outland.js"
    },

    "outland_pokemon_title": {
      "title": "Окно-подсказка с параметрами покемонов",
      "file": "outland_pokemon_title.js",
      "configure": {
        "quantity": {
          "type": "checkbox",
          "title": "Количество",
          "default": true
        },
        "power": {
          "type": "checkbox",
          "title": "Мощность",
          "default": true
        },
        "prize": {
          "type": "checkbox",
          "title": "Премия",
          "default": true
        }
      }
    }
  }
}