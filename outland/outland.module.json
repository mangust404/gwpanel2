{
  "requires": ["items", "map"], 
  "excludes": "",
  "title": "Модуль для Outland",
  "weight": 5, 
  "apply": {
    "/walk.php": ["outland_check_set", "outland_auto_sail", "outland_auto_close", "outland_auto_pick", "outland_highlight", "outland_hotkeys"],
    "/walk.op.php": ["outland_check_set", "outland_auto_sail", "outland_auto_close", "outland_auto_pick", "outland_highlight", "outland_hotkeys"],
    "/walk.ep.php": ["outland_check_set", "outland_auto_sail", "outland_auto_close", "outland_auto_pick", "outland_highlight", "outland_hotkeys"]
  },
  "settings": {
    "outland_check_set": {
      "configure": {
        "set": {
          "type": "select",
          "title": "комплект",
          "options": ["test1", "test2"],
          "options": "__panel.items_get_sets_async()",
          "file": "items/items_data.js",
          "default": 0
        }
      },
      "file": "outland.js"
    },

    "outland_auto_sail": {
      "description": "Автоматически отплывать, ага",
      "configure": {
        "port": {
          "type": "select",
          "title": "порт назначения",
          "options": "__panel.map_get_ports_ids()",
          "default": "",
          "file": "map/map_sectors.js"
        }
      },
      "file": "outland.js"
    },

    "outland_auto_close": {
      "description": "Автоматически закрывать заявку",
      "file": "outland.js"
    },

    "outland_auto_pick": {
      "description": "Автоматически подбирать вещи",
      "file": "outland.js"
    },

    "outland_highlight": {
      "description": "Подкрашивать клетки, на которые можно перейти",
      "file": "outland.js"
    },

    "outland_hotkeys": {
      "description": "Быстрое передвижение стрелками на клавиатуре",
      "file": "outland.js"
    }    
  }
}