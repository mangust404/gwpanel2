{
  "requires": ["items", "map"], 
  "excludes": "",
  "title": "Модуль для Outland",
  "weight": 5, 
  "apply": {
    "/walk.php": ["outland_init"],
    "/walk.op.php": ["outland_init"],
    "/walk.ep.php": ["outland_init"]
  },
  "settings": {
    "outland_init": {
      "description": "основные функции на ауте",
      "file": "outland.js",
      "configure": {
        "check_set": {
          "type": "select",
          "title": "комплект",
          "options": ["test1", "test2"],
          "options_file": "items/items.js",
          "default": 0
        },
        "autos": {
          "type": "checkbox",
          "title": "Автоматически отплывать, ага",
          "default": 0, 
          "description": "В сочетании с предыдущей настройкой при нажатии на якорь само отплывает ;-}"
        },
        "fightas": {
          "type": "checkbox",
          "title": "Автоматически закрывать заявку",
          "default": 0, 
          "description": "Автоматом закрывает заявку как только вошли"
        },
        "takeas": {
          "type": "checkbox",
          "title": "Автоматически подбирать вещи",
          "default": 0, 
          "description": "Автоматом подбирает предмет, как только оказываемся рядом"
        },
        "highlight": {
          "type": "checkbox",
          "title": "Подкрашивать клетки, на которые можно перейти",
          "default": 1, 
          "description": "Слегка затемняет фон кликабельных клеток"
        }
      }
    }
  }
}